{
  "name": "naive-potato-ui",
  "version": "0.0.1",
  "main": "",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter @naive-potato-ui/examples dev",
    "clean:type": "rimraf ./dist",
    "type:node": "tsc -p tsconfig.node.json --noEmit --composite false",
    "type:src": "pnpm run clean:type && vue-tsc -p tsconfig.src.json --composite false --declaration --emitDeclarationOnly",
    "mv-type": "tsx ./scripts/dts-mv.ts",
    "build:ui": "pnpm run type:src && pnpm --filter ./packages/** run build && pnpm run mv-type"
  },
  "devDependencies": {
    "@inquirer/prompts": "^3.1.2",
    "@types/lodash": "^4.14.199",
    "@types/node": "^20.6.3",
    "@vitejs/plugin-vue": "^4.2.3",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "lodash": "^4.17.21",
    "log-symbols": "^5.1.0",
    "naive-ui": "^2.34.4",
    "ora": "^7.0.1",
    "rimraf": "^5.0.5",
    "tsx": "^3.13.0",
    "typescript": "^5.0.2",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.5",
    "vite-plugin-windicss": "^1.9.1",
    "vue-tsc": "^1.8.5",
    "windicss": "^3.5.6"
  },
  "dependencies": {
    "vue": "^3.3.4"
  }
}